    "dotenv": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-react-app": "^0.6.2",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.11.1",
    "fs-extra": "^2.1.2",
